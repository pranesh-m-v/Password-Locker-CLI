//saves password in passwordArray
void savePassword(char password[])
{
    int count = 0;
    while(strlen(passwordArray[count]))
    {
        count++;
    }
    for(int i = 0; i < strlen(password); i++)
    {
        passwordArray[count][i] = password[i]+i;
    }
    //puts(passwordArray[count]);
}
